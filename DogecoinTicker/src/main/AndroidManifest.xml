<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="christophior.dogecointicker.dogecointicker" >

    <!-- Internet permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Settings"
            android:label="Settings">
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.ExchangeList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.cryptsy"
            android:label="@string/title_activity_cryptsy" >
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.coinedup"
            android:label="@string/title_activity_coinedup" >
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.coinse"
            android:label="@string/title_activity_coinse" >
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.bter"
            android:label="@string/title_activity_bter" >
        </activity>
        <activity
            android:name="christophior.dogecointicker.dogecointicker.vircurex"
            android:label="@string/title_activity_vircurex" >
        </activity>
    </application>

</manifest>
